# Aliases created by the command save bash function, als

alias rmbr="git push origin --delete"

alias rmbl="git branch -D"

alias hp="git push heroku master"

alias compress="tar czf"

alias grh="git reset --hard"

alias unstage="git reset HEAD"

alias grs="git reset --soft HEAD~"

alias archive="tar -cvzf"

alias dbs="sudo mysql.server start"

alias gds="git diff --staged"

alias gcm="gc -m"

alias jd-deploy="gco release && git merge develop && git push && npm run deploy && gco develop"

alias t-deploy="gco gh-pages && git merge master && git push && gco master"

alias sq-deploy="gco master && git merge develop && git push && gco develop"
